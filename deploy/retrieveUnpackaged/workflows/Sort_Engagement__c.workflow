            <field>Sort_Engagement__c.Deal_Center_Status__c</field>
            <operation>equals</operation>
            <value>Technical Review</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Owner_Role__c</field>
            <operation>notContain</operation>
            <value>EMEA,Benelux</value>
        </criteriaItems>
        <criteriaItems>
            <field>Sort_Engagement__c.Sales_Team__c</field>
            <operation>contains</operation>
            <value>US Acq,US Emerging</value>
        </criteriaItems>
        <criteriaItems>
            <field>Sort_Engagement__c.Sales_Team__c</field>
            <operation>notContain</operation>
            <value>Acq MVT</value>
        </criteriaItems>
        <description>When the Deal Center Status = Technical initially, alerts are sent to the appropriate Acq SEs.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
