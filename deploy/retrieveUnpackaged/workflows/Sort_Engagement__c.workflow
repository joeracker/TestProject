    <rules>
        <fullName>Deal Center Opp Is Closed Lost</fullName>
        <actions>
            <name>An_Opportunity_Containing_a_Deal_Center_Request_has_been_Closed_Lost</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Sort_Engagement__c.CreatedDate</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Owner_Role__c</field>
            <operation>notContain</operation>
            <value>EMEA,Benelux</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Lost</value>
        </criteriaItems>
        <description>alerts Deal Center team that an opp containing a Deal Center request has been closed lost.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
