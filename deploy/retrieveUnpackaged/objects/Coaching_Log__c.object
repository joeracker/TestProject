    <validationRules>
        <fullName>Must_insert_duration_when_completing</fullName>
        <active>true</active>
        <description>The duration must be populated when the status is completed</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c, &quot;Completed&quot;), OR(Duration__c = 0.00, ISNULL(Duration__c)))</errorConditionFormula>
        <errorMessage>You must enter a duration before saving with a status of completed.</errorMessage>
    </validationRules>
