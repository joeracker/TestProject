    <deploymentStatus>Deployed</deploymentStatus>
    <enableActivities>true</enableActivities>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Activity_Type__c</fullName>
        <description>Denotes the type of activity performed.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please select the type of activity to be performed.</inlineHelpText>
        <label>Activity Type</label>
        <picklist>
            <picklistValues>
                <fullName>1x1</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Call Monitoring</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Chat Monitoring</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Feedback</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Side by side</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>true</trackHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Completed_Date__c</fullName>
        <description>Denotes the completed date.</description>
        <externalId>false</externalId>
        <inlineHelpText>This date is automatically populated when the log is saved as completed.</inlineHelpText>
        <label>Completed Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Duration__c</fullName>
        <description>Denotes the duration of the coaching.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please enter a duration when you have completed your coaching.</inlineHelpText>
        <label>Duration</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>true</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Manager__c</fullName>
        <description>Denotes the rep who was coached.</description>
        <externalId>false</externalId>
        <inlineHelpText>Select the rep who this coaching log is with.</inlineHelpText>
        <label>Manager</label>
        <referenceTo>User</referenceTo>
        <relationshipName>R00N50000001nvHQEAY</relationshipName>
        <trackHistory>true</trackHistory>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Notes__c</fullName>
        <description>Notes</description>
        <externalId>false</externalId>
        <inlineHelpText>Please enter the notes from your coaching session.</inlineHelpText>
        <label>Notes</label>
        <length>32000</length>
        <trackHistory>false</trackHistory>
        <type>LongTextArea</type>
        <visibleLines>10</visibleLines>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>Denotes the status of the coaching log.</description>
        <externalId>false</externalId>
        <inlineHelpText>Please select a status.  When completed is selected the completed date will populate and the duration will become required. The rep and owner (manager) will receive email notification of the completion.</inlineHelpText>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>New</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>In Progress</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>On Hold</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Completed</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackHistory>true</trackHistory>
        <type>Picklist</type>
    </fields>
    <label>Coaching Log</label>
    <listViews>
        <fullName>All</fullName>
        <columns>Manager__c</columns>
        <columns>Completed_Date__c</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>NAME</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <nameField>
        <label>Coaching Log Name</label>
        <trackHistory>false</trackHistory>
        <type>Text</type>
    </nameField>
    <pluralLabel>Coaching Logs</pluralLabel>
