    <deploymentStatus>Deployed</deploymentStatus>
    <description>Details of the request Including: partner account, marketing activity, proposed activity date, requested amount, approved amount, and percentage approved.</description>
    <enableActivities>true</enableActivities>
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <externalId>false</externalId>
        <label>Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Activity_Date__c</fullName>
        <externalId>false</externalId>
        <label>Activity Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Activity_Type__c</fullName>
        <externalId>false</externalId>
        <label>Activity Type</label>
        <picklist>
            <picklistValues>
                <fullName>Advertising</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Direct Mail</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Sales Tools</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Seminar</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Tradeshow</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>None</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Age_Days__c</fullName>
        <externalId>false</externalId>
        <formula>NOW() - CreatedDate</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Days)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Amount__c</fullName>
        <externalId>false</externalId>
        <label>Requested Amount</label>
        <precision>12</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Approval_History__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK(&quot;/00O50000001DEpq?pv0=&quot; &amp; Id , &quot;View Approval History&quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval History</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Approved_Amount__c</fullName>
        <externalId>false</externalId>
        <label>Approved Amount</label>
        <precision>8</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Date_Approved__c</fullName>
        <externalId>false</externalId>
        <label>Date Approved</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Desired_Outcome__c</fullName>
        <externalId>false</externalId>
        <label>Desired Outcome</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Partner_Comments__c</fullName>
        <externalId>false</externalId>
        <label>Partner Comments</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Partner_Contribution__c</fullName>
        <description>The amount the partner is contributing to this effort. This is in addition to the amount requested of the vendor.</description>
        <externalId>false</externalId>
        <label>Partner Contribution</label>
        <precision>10</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Percent_Approved__c</fullName>
        <externalId>false</externalId>
        <formula>Approved_Amount__c / Amount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>% Approved</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Request_Details__c</fullName>
        <externalId>false</externalId>
        <label>Request Details</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Request_Name__c</fullName>
        <description>Optional field which is more descriptive than the automatically assigned request number</description>
        <externalId>false</externalId>
        <label>Request Name</label>
        <length>100</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>S1__c</fullName>
        <description>Stores the servlet location which is substituted in formula fields.</description>
        <externalId>false</externalId>
        <formula>&quot;/servlet/servlet.Integration?lid=&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>s_Servlet Location</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>S2__c</fullName>
        <externalId>false</externalId>
        <formula>&quot;/servlet/servlet.FileDownload?file=&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>s_Document Directory</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SFDC_Channel_Budget__c</fullName>
        <description>Optionally assign the fund request to a channel budget.</description>
        <externalId>false</externalId>
        <label>Channel Budget</label>
        <referenceTo>SFDC_Channel_Budget__c</referenceTo>
        <relationshipName>R00N50000001NBxqEAG</relationshipName>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Status_Action__c</fullName>
        <externalId>false</externalId>
        <formula>case(Status__c , &quot;&quot;,  HYPERLINK(S1__c&amp;&quot;01N500000004dkw&amp;eid=&quot;&amp;Id, IMAGE(S2__c&amp;&quot;01550000000BPMn&quot;, &quot;Submit&quot;) ), &quot;Submitted&quot;, HYPERLINK(S1__c&amp;&quot;01N500000004dkt&amp;eid=&quot;&amp;Id, IMAGE(S2__c&amp;&quot;01550000000BPMl&quot;, &quot;Approve&quot;) )  &amp;  HYPERLINK(S1__c&amp;&quot;01N500000004dl0&amp;eid=&quot;&amp;Id, IMAGE(S2__c&amp;&quot;01550000000BPMm&quot;, &quot;Reject&quot;) ), &quot;Approved&quot;, HYPERLINK(S1__c&amp;&quot;01N500000004dl0&amp;eid=&quot;&amp;Id, IMAGE(S2__c&amp;&quot;01550000000BPMm&quot;, &quot;Reject&quot;) ), &quot;Rejected&quot;, HYPERLINK(S1__c&amp;&quot;01N500000004dkw&amp;eid=&quot;&amp;Id, IMAGE(S2__c&amp;&quot;01550000000BPMn&quot;, &quot;Submit&quot;) ), &quot;N/A&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Action</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <label>Approval Status</label>
        <picklist>
            <picklistValues>
                <fullName>Submitted</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Approved</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Not Approved</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>account__c</fullName>
        <externalId>false</externalId>
        <label>Partner Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipName>R00N50000001NBx4EAG</relationshipName>
        <type>Lookup</type>
    </fields>
