    <fields>
        <fullName>NPS_Decision_Maker__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>This is used to send the NPS Survey to the decision maker of the account.  The contact does not have to be available for access to the account.  Be sure to select NPS Contact ONLY as the drop down role column if they should not have access to the account.</inlineHelpText>
        <label>NPS Decision Maker</label>
        <trackHistory>true</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NPS_INT_Business_Contact__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
