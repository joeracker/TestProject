        <inlineHelpText>This is used to send the NPS Survey to the billing contact of the account. The contact does not have to be available for access to the account. Be sure to select NPS Contact ONLY as the drop down role column if they should not have access to the account.</inlineHelpText>
        <label>NPS MGD Billing Contact</label>
        <trackHistory>true</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NPS_Survey__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Denotes the contacts that will receive the NPS Survey from EMEA.</description>
        <externalId>false</externalId>
        <inlineHelpText>Denotes the contacts that will receive the NPS Survey from EMEA.</inlineHelpText>
        <label>EMEA NPS Survey</label>
        <trackHistory>true</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NPS_Technical_Contact__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>This is used to send the NPS Survey to the technical contact of the account. The contact doesn&apos;t have to be available for access to the account. Be sure to select NPS Contact ONLY as the drop down role column if they should not have access to the account.</inlineHelpText>
        <label>NPS Technical Contact</label>
        <trackHistory>true</trackHistory>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>NPS__c</fullName>
        <externalId>false</externalId>
        <formula>If(NPS_Decision_Maker__c, 1, 0) + if( NPS_INT_Business_Contact__c, 1, 0) + if( NPS_MGD_Billing_Contact__c, 1, 0) + if( NPS_Technical_Contact__c, 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>NPS</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
