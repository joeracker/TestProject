    <validationRules>
        <fullName>Only_allow_one_primary_contact</fullName>
        <active>false</active>
        <description>Only allows one primary contact.</description>
        <errorConditionFormula>AND(Rackspace_Account__r.Primary_Count__c &gt;=1,  ISPICKVAL(Role__c, &apos;Primary Contact&apos;))</errorConditionFormula>
        <errorDisplayField>Role__c</errorDisplayField>
        <errorMessage>You can only have one Primary Contact on an account. Please edit or mark the existing Primary Contact for deletion.</errorMessage>
    </validationRules>
