    <validationRules>
        <fullName>Cannot_update_Survey_Name</fullName>
        <active>true</active>
        <description>Users cannot update survey names - will mess up reporting</description>
        <errorConditionFormula>AND(ISCHANGED(Name),
NOT($Profile.Name = &quot;System Administrator&quot;))</errorConditionFormula>
        <errorMessage>You cannot change the Survey Name.  This will cause reporting issues.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Created_CORE_Ticket</fullName>
        <availability>online</availability>
        <description>Used to create a NPS ticket in CORE</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Created CORE Ticket</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/8.0/connection.js&quot;)} 

var nps = new sforce.SObject(&quot;NPS_Survey__c&quot;); 
nps.id = &quot;{!NPS_Survey__c.Id}&quot;; 
nps.ticket_created__c = true; 
nps.ticket_creation_status__c = &quot;Ticket is being created in CORE. Please do not click button again.&quot;; 

var result = sforce.connection.update([nps]); 

if (result[0].getBoolean(&quot;success&quot;)) 
{ 
// Refresh window 
window.location.reload(); 
} 
else 
{ 
alert(&quot;Error saving NPS&quot;); 
}</url>
    </webLinks>
