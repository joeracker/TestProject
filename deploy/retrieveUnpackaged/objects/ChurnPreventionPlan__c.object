    <validationRules>
        <fullName>Cloud_Required_Fields</fullName>
        <active>true</active>
        <description>If the CLOUD check box is selected then all CLOUD fields are required.</description>
        <errorConditionFormula>AND(CLOUD__c,  
OR(ISPICKVAL(Forced_to_deploy_additional_servers__c, &quot;&quot;),
ISPICKVAL(Significant_web_server_customization__c, &quot;&quot;),
ISPICKVAL(Foreced_to_purchase_bandwidth__c, &quot;&quot;),
ISNULL(What_are_the_compliance_needs__c),
LEN(What_are_the_compliance_needs__c)&lt;1,
ISNULL(What_programming_language_being_used__c),
LEN(What_programming_language_being_used__c)&lt;1,
ISNULL(What_database_platform_is_used__c),
LEN(What_database_platform_is_used__c)&lt;1,
ISNULL(What_programming_libraries_are_used__c),
LEN(What_programming_libraries_are_used__c)&lt;1,
ISNULL(Web_Server_Customization_Details__c),
LEN(Web_Server_Customization_Details__c)&lt;1))</errorConditionFormula>
        <errorMessage>All fields in the CLOUD section are required when CLOUD is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Devices_being_added_required</fullName>
        <active>true</active>
        <description>When Devices being added = Yes then exact configuration and pricing is required.</description>
        <errorConditionFormula>AND(ISPICKVAL(Devices_being_added__c, &quot;Yes&quot;),
OR(LEN(What_s_the_exact_configuration__c) &lt; 1,
ISNULL(What_s_the_exact_configuration__c), 
LEN(Pricing_for_devices_being_added__c) &lt; 1,
ISNULL(Pricing_for_devices_being_added__c)))</errorConditionFormula>
        <errorDisplayField>Devices_being_added__c</errorDisplayField>
        <errorMessage>When Devices being added = Yes then What &apos;s the exact configuration and Pricing for devices being added are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Devices_being_decommissioned_required</fullName>
        <active>true</active>
        <description>When Devices being decommissioned = yes the decommissioned devices and the pricing for decommissioned devices are required.</description>
        <errorConditionFormula>AND(ISPICKVAL(Devices_being_decommissioned__c, &quot;Yes&quot;),
OR(LEN(Decommissioned_Devices__c) &lt;1,
ISNULL(Decommissioned_Devices__c), 
LEN(Pricing_for_decommissioned_devices__c) &lt; 1,
ISNULL(Pricing_for_decommissioned_devices__c)))</errorConditionFormula>
        <errorDisplayField>Devices_being_decommissioned__c</errorDisplayField>
        <errorMessage>When Devices being commissioned = Yes then Decommissioned Devices and Pricing for decommissioned devices are required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Finance_all_are_required</fullName>
        <active>true</active>
        <description>If the Finance checkbox is checked then all fields on the right in the Finance section are required.</description>
        <errorConditionFormula>AND(FINANCE__c,
OR(ISPICKVAL(Double_Check_Configuration_in_CORE__c, &quot;&quot;),
ISPICKVAL(Pricing_tool_match_invoice__c, &quot;&quot;),
ISPICKVAL(Has_account_done_prepays_in_the_past__c, &quot;&quot;),
ISPICKVAL(Devices_being_decommissioned__c, &quot;&quot;),
ISPICKVAL(Devices_being_added__c, &quot;&quot;),
ISPICKVAL(Would_customer_like_pricing__c, &quot;&quot;),
ISNULL(Customer_s_future_plan_with_Rackspace__c)))</errorConditionFormula>
        <errorMessage>All fields on the left side in the FINANCE section are required when FINANCE is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Has_account_done_prepays_required</fullName>
        <active>true</active>
        <description>When Has account done prepays = Yes then the fields on the right are required (previous amount, when paid, when end, monthly fee be)</description>
        <errorConditionFormula>AND(ISPICKVAL( Has_account_done_prepays_in_the_past__c, &quot;Yes&quot;), 
OR(Previous_prepay_amount__c = 0.00,
ISNULL(Previous_prepay_amount__c), 
ISNULL(When_did_they_pay__c), 
ISNULL(When_will_it_end__c), 
What_should_be_their_monthly_fee__c = 0.00,
ISNULL(What_should_be_their_monthly_fee__c)))</errorConditionFormula>
        <errorDisplayField>Has_account_done_prepays_in_the_past__c</errorDisplayField>
        <errorMessage>If Has account done prepays in the past = Yes then you must populate Previous Prepay Amount, When did they pay, When will it end, and What should be their monthly fee.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Legal_all_are_required</fullName>
        <active>true</active>
        <description>When the Legal checkbox is checked the Legal fields are required.</description>
        <errorConditionFormula>AND( LEGAL__c,  LEN(Rack_Law_Ticket__c)&gt;1)</errorConditionFormula>
        <errorMessage>All fields in the LEGAL section are required when LEGAL is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Platform_all_are_required</fullName>
        <active>true</active>
        <description>If the Platform checkbox is checked all Platform section fields are required.</description>
        <errorConditionFormula>AND(PLATFORM__c,
OR(ISPICKVAL(At_least_5_servers__c, &quot;&quot;),
ISPICKVAL(Capability_to_fully_support_OS__c, &quot;&quot;),
ISPICKVAL(Any_licensing_needs_beyond_the_OS__c, &quot;&quot;),
ISPICKVAL(Can_Customer_support_network__c, &quot;&quot;),
ISPICKVAL(Can_customer_respond_to_app_issues_24_7__c, &quot;&quot;),
ISPICKVAL(Can_customer_manage_all_backups__c, &quot;&quot;),
ISPICKVAL(Special_deals_with_customer__c, &quot;&quot;),
ISPICKVAL(Has_an_SE_qualified_acct_for_platform__c, &quot;&quot;),
ISPICKVAL(Compiled_config_capex_in_pricing_tool__c, &quot;&quot;), 
ISNULL(SE_Name__c),
ISNULL(BDC_Name__c)))</errorConditionFormula>
        <errorMessage>All fields in the PLATFORM section are required when PLATFORM is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Tool_Match_Required</fullName>
        <active>true</active>
