    <validationRules>
        <fullName>ASA_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Hybrid_Offering__c, &quot;Cloud Connect ASA&quot;),
OR(TEXT(ASA_Discussion_ASA_Model__c) = &quot;&quot;,
TEXT(ASA_Discussion_Load_Balancing_Complex__c) = &quot;&quot;))</errorConditionFormula>
        <errorMessage>You selected ASA from Hybrid Offering field therefore you must enter values in the ASA Model and ASA Load Balancing Complex fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F5_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Hybrid_Offering__c, &quot;Cloud Connect F5&quot;),
OR(TEXT(F5_Discussion_F5_Code_Version__c) = &quot;&quot;,
TEXT(F5_Discussion_Offering_Capabilities__c) = &quot;&quot;))</errorConditionFormula>
        <errorMessage>You selected F5 from Hybrid Offering field therefore you must enter values in the F5 Code Version and F5 Offering Capabilities fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>F5_and_ASA_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Hybrid_Offering__c, &quot;Cloud for Enterprises&quot;)),
OR(TEXT( F5_and_ASA_Discussion_Access_Req_Process__c)  = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Cloud_Net_Changes__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Cloud_Throughput__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Host_Bas__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Managed_Colo__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Minimum_Deal_Size__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_PATing__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Support_Levels__c) = &quot;&quot;,
TEXT(F5_and_ASA_Discussion_Cloud_Data_Center__c) = &quot;&quot;))</errorConditionFormula>
        <errorMessage>You selected F5 or ASA from Hybrid Offering field therefore you must enter values in the in all of the F5 and ASA fields.</errorMessage>
    </validationRules>
</CustomObject>
